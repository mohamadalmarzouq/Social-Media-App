{
  "name": "social-media-contest-app",
  "description": "Contest-based marketplace for social media content creation",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "npm install",
    "build": "npm install && npx prisma generate && next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate deploy",
    "test:deployment": "node scripts/test-deployment.js",
    "render:check": "node scripts/render-deploy-check.js",
    "debug:render": "node scripts/debug-render.js",
    "clean:build": "rm -rf .next && npm run build",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^5.22.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "multer": "^1.4.5-lts.1",
    "next": "15.4.6",
    "next-auth": "^4.24.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.53.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "typescript": "^5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.12",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^5.22.0"
  }
}
